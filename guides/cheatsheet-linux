echo $XDG_SESSION_TYPE       # check X11 or wayland
sudo fwupdmgr update              # check system boottime
systemd-analyze time                # display information of the system
uname -a                            # for all the stuff
alias ls="ls --color=auto"          # link bash cmd with config
mkdir -p {}                       # create multiple directory within {}
sudo systemctl enable --now [service] # enable a service
sudo systemctl status [service] # check service status
head and tail -n 5
journalctl
touch [1..3].txt
ln -s create link
find -name -type
finger
tac and cat 
# same but reverse order of display
ncdu # gui disk analyzer
route -n # show IP only
source bash script for it to take affect
source ~/.config/fish/config.fish # fish requires different script file, not bash
